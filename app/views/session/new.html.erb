<% if @errors %>
  <% @errors.each do |error| %>
    <p> <%= error %></p>
  <% end %>
<% end %>

<%= form_for :session, url: session_index_path do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email, placeholder: "email@wbhs@.edu" %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Log In" %>
  </p>
<% end %>
