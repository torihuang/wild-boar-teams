<% if @errors %>
  <% @errors.each do |error| %>
    <p> <%= error %></p>
  <% end %>
<% end %>

<%= form_for :session, url: session_index_path do |f| %>
  <div class="form-group">
    <%= f.label :email %><br>
    <%= f.email_field :email, placeholder: "email@wbhs.edu" %>
  </div>

  <div class="form-group">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>

  <p>
    <%= f.submit "Log In", class: "btn btn-default" %>
  </p>
<% end %>
