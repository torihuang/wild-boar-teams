<%= form_for :users do |f| %>

  <div class="form-group">
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </div>
  <div class="form-group">
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </div>
  <div class="form-group">
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </div>
  <div class="form-group">
    <%= f.label :image_path %><br>
    <%= f.text_field :image_path %>
  </div>
  <div class="form-group">
    <%= f.label :master_password %><br>
    <%= f.password_field :master_password %>
  </div>
  <div class="actions">
    <%= f.submit "Create New Teacher", class: "btn btn-default" %>
  </div>
<% end %>
